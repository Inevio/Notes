
{

    "name"    : "Notes",
    "version" : "1.0",
    "stage"   : "beta",
    "api"     : "a100d2",

    "language" : {

        "default" : "en-en",
        "list"    : [ "en-en" ]

    },

    "device" : {

        "desktop" : {

            "primary" : "main",

            "common"  : {

                "css"    : [],
