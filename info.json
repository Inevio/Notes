            },

            "view" : {

                "main" : {

                    "template" : "main.html",

                    "maxizable" : false,
                    "resizable" : false,

                    "width"  : {

                        "default" : 790,
                        "min"     : 0,
                        "max"     : 0

                    },

                    "height" : {

                        "default" : 550,
                        "min"     : 0,
                        "max"     : 0

                    },

                    "css"    : [ "style.css" ],
                    "script" : [ "script/build/desktop.js" ],
                    "start"  : [ "start.js" ]

                }

            }

        },

        "mobile" : {

            "primary" : "main",

            "common"  : {

                "css"    : [],
                "script" : [],
                "start"  : []

            },

            "view" : {

                "main" : {

                    "template" : "mainMobile.html",

                    "css"    : [ "style.css" ],
                    "script" : [ "script.js" ],
                    "start"  : [ "start.js" ]


                }

            }

        }

    },

    "permission" : {

        "user" : {

            "read" : true

        }

